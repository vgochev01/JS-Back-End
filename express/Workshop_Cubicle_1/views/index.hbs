<h1>Browser</h1>
<form action="" method="">
  <input type="text" class="search" name="search" placeholder="Search..." />
  <input
    type="number"
    name="from"
    class="difficulty"
    placeholder="Difficulty from..."
  />
  <span>-</span>
  <input
    type="number"
    name="to"
    class="difficulty"
    placeholder="Difficulty to..."
  />
  <input type="submit" value="search" />
</form>

{{#if cubes}}
  {{#each cubes}}
    {{> cube}}
  {{/each}}
{{else}}
<span class="emptyMsg">No Cubes in Database &RightArrow; <a href="/create">Create</a> the first one!</span>
{{/if}}